<?xml version="1.0"?>
<mime-info xmlns='http://www.freedesktop.org/standards/shared-mime-info'>
    <mime-type type="application/x-dotx">
        <comment>DotX Project File</comment>
        <comment xml:lang="en">DotX Project File</comment>
        <icon name="dotx"/>
        <glob-deleteall/>
        <glob pattern="*.dotx"/>
        <magic priority="50">
            <match value="DotX" type="string" offset="0"/>
        </magic>
    </mime-type>
    
    <mime-type type="application/x-fasta">
        <comment>FASTA Sequence File</comment>
        <comment xml:lang="en">FASTA Sequence File</comment>
        <glob pattern="*.fasta"/>
        <glob pattern="*.fas"/>
        <glob pattern="*.fa"/>
        <glob pattern="*.fna"/>
        <glob pattern="*.ffn"/>
        <glob pattern="*.faa"/>
        <glob pattern="*.frn"/>
        <magic priority="50">
            <match value="&gt;" type="string" offset="0"/>
        </magic>
    </mime-type>
    
    <mime-type type="application/x-paf">
        <comment>PAF Alignment File</comment>
        <comment xml:lang="en">PAF Alignment File</comment>
        <glob pattern="*.paf"/>
    </mime-type>
</mime-info>